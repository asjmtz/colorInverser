{
  "manifest_version": 2,

  "name": "丑丑的网页反色器",
  "description": "将网页的设置为灰底白字，网页不会看得眼花目眩",
  "version": "0.1.0",


  "icons": {
    "16": "img/icon_16.png",
    "48": "img/icon_16.png",
    "128": "img/icon_16.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "img/icon_16.png"
  },
  "permissions": [
    "activeTab"
  ]
}